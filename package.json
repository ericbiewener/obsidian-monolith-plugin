{
  "repository": {
    "type": "git",
    "url": "https://github.com/ericbiewener/obsidian-plugin-monorepo"
  },
  "scripts": {
    "start": "webpack --watch --env",
    "start2": "tsx webpack/build-plugin.bin.ts",
    "tc": "tsc --noEmit --watch",
    "build": "rm -rf dist && NODE_ENV=production webpack --env",
    "build:all": "scripts/build-all-plugins.sh",
    "biome": "biome check --apply --no-errors-on-unmatched",
    "biome:all": "pnpm biome **/*",
    "prettier": "prettier --write",
    "prettier:all": "pnpm prettier **/*.md",
    "format": "pnpm biome:all && pnpm prettier:all",
    "prepare": "husky install",
    "gen:plugin": "tsx src/plugin-generator/index.ts",
    "gen:style-types": "tcm src"
  },
  "keywords": [],
  "author": "Eric Biewener",
  "license": "MIT",
  "version": "1.3.0",
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@codemirror/view": "6.26.0",
    "@types/fs-extra": "11.0.1",
    "@types/inquirer": "9.0.3",
    "@types/node": "20.4.8",
    "@types/yargs": "17.0.32",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "@typescript-eslint/parser": "6.2.1",
    "builtin-modules": "3.3.0",
    "chalk": "4.1.2",
    "compare-versions": "6.0.0",
    "css-loader": "6.8.1",
    "esbuild": "0.18.18",
    "execa": "5.1.1",
    "fs-extra": "11.1.1",
    "globby": "11.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "obsidian": "1.4.0",
    "open": "9.1.0",
    "prettier": "3.2.5",
    "prettier-plugin-organize-imports": "4.1.0",
    "style-loader": "3.3.3",
    "terser-webpack-plugin": "5.3.9",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.2",
    "tslib": "2.6.1",
    "tsx": "4.7.1",
    "type-fest": "4.12.0",
    "typed-css-modules": "0.9.1",
    "typescript": "5.1.6",
    "uglify-js": "3.17.4",
    "utility-types": "3.10.0",
    "webpack": "5.88.2",
    "webpack-cli": "5.1.4",
    "yargs": "17.7.2"
  },
  "lint-staged": {
    "*.md": "pnpm prettier",
    "*.{ts,js,json}": "pnpm biome"
  },
  "dependencies": {
    "case-anything": "2.1.13",
    "date-fns": "2.30.0",
    "debounce": "2.0.0",
    "fuzzy": "0.1.3"
  },
  "engines": {
    "node": "20.12.0"
  }
}

{
  "repository": {
    "type": "git",
    "url": "https://github.com/ericbiewener/obsidian-plugin-monorepo"
  },
  "scripts": {
    "start": "webpack --watch --env",
    "tc": "tsc --noEmit --watch",
    "build": "rm -rf dist && NODE_ENV=production webpack --env",
    "build:all": "scripts/build-all-plugins.sh",
    "biome": "biome check --apply --no-errors-on-unmatched --log-level=info **/*",
    "prettier": "prettier --write **/*.md",
    "format": "pnpm biome && pnpm prettier",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Eric Biewener",
  "license": "MIT",
  "version": "1.3.0",
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@types/fs-extra": "11.0.1",
    "@types/inquirer": "9.0.3",
    "@types/node": "20.4.8",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "@typescript-eslint/parser": "6.2.1",
    "builtin-modules": "3.3.0",
    "chalk": "4.1.2",
    "compare-versions": "6.0.0",
    "css-loader": "6.8.1",
    "esbuild": "0.18.18",
    "execa": "5.1.1",
    "fs-extra": "11.1.1",
    "globby": "11.1.0",
    "husky": "^8.0.3",
    "inquirer": "8.2.6",
    "lint-staged": "^13.2.3",
    "obsidian": "1.4.0",
    "open": "9.1.0",
    "prettier": "3.2.5",
    "style-loader": "3.3.3",
    "terser-webpack-plugin": "5.3.9",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tslib": "2.6.1",
    "typescript": "5.1.6",
    "uglify-js": "3.17.4",
    "utility-types": "3.10.0",
    "webpack": "5.88.2",
    "webpack-cli": "5.1.4"
  },
  "lint-staged": {
    "*.md": "prettier --write",
    "*.{ts,js,json}": "biome check --apply"
  },
  "dependencies": {
    "date-fns": "2.30.0",
    "fuzzy": "0.1.3",
    "param-case": "3.0.4"
  },
  "engines": {
    "node": "20.10.0"
  }
}
